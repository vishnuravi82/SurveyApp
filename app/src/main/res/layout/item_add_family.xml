                     android:layout_marginTop="8dp"
                     android:layout_marginEnd="8dp"
                     android:layout_marginBottom="8dp"
                     android:background="@drawable/spinnerbg"
                     android:spinnerMode="dropdown" />
-->

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/label11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="0dp"
                    android:tag="@string/brandon_reg"
                    android:text="@string/h_RelationShip"
                    android:textColor="@color/black"
                     />

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0dp">

                    <EditText
                        android:id="@+id/et11"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:digits="@string/allowed_char_default"
                        android:inputType="text"
                        android:background="@color/white"
                        android:lines="1"
                        android:tag="@string/brandon_light"
                        android:textColor="@color/black"
                         />
                </com.google.android.material.textfield.TextInputLayout>


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/label21"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:layout_marginTop="15dp"

                    android:tag="@string/brandon_reg"
                    android:text="@string/h_Occupation"
                    android:textColor="@color/black"
                    android:textSize="@dimen/label_size" />

                <Spinner
                    android:id="@+id/sp_ocupation"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/spinner_height"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginBottom="8dp"
                    android:background="@drawable/spinnerbg"
                    android:spinnerMode="dropdown" />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/labelMNRG"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:tag="@string/brandon_reg"
                    android:text="@string/q_mn_card"

                    android:textColor="@color/black"
                    android:textSize="@dimen/label_size" />

                <Spinner
                    android:id="@+id/sp_Mnrg"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/spinner_height"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginBottom="8dp"
                    android:background="@drawable/spinnerbg"
                    android:spinnerMode="dropdown" />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/label_card"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:tag="@string/brandon_reg"
                    android:text="@string/q_card_no"
                    android:visibility="gone"

                    android:textColor="@color/black"
                    android:textSize="@dimen/label_size" />

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:layout_marginTop="0dp">

                    <EditText
                        android:id="@+id/et_card12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:inputType="number"
                        android:lines="1"
                        android:tag="@string/brandon_light"
                        android:textColor="@color/black"
                        android:textSize="@dimen/input_size" />
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>

</LinearLayout>